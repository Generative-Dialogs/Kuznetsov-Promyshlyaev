00215 {\cf22         @return Tuple[Optional[List[CommandData]], str] \'CA\'EE\'F0\'F2\'E5\'E6 \'E8\'E7 \'F1\'EF\'E8\'F1\'EA\'E0 \'EA\'EE\'EC\'E0\'ED\'E4 (\'E8\'EB\'E8 None) \'E8 \'F1\'EE\'EE\'E1\'F9\'E5\'ED\'E8\'FF \'EE\'E1 \'EE\'F8\'E8\'E1\'EA\'E5}\par
00216 {\cf22         """}\par
00217         cleaned_text = {\cf22 "\\n"}.join([line {\cf19 for} line {\cf19 in} input_text.splitlines() {\cf19 if} line.strip()])\par
00218         lines = cleaned_text.strip().split({\cf22 "\\n"})\par
00219         i = 0\par
00220         parsed_data: List[CommandData] = []\par
00221         tmp_characters_names: List[str] = []\par
00222         {\cf19 while} i < len(lines):\par
00223             line = lines[i].strip()\par
00224 \par
00225             {\cf19 if} line == {\cf22 "Create character command."}:\par
00226                 {\cf19 if} i + 2 >= len(lines):\par
00227                     {\cf19 return} {\cf18 None}, {\cf22 "Error: Incomplete 'Create a character command.'"}\par
00228                 name = lines[i + 1].strip()\par
00229                 {\cf19 if} name {\cf19 in} self.characters {\cf19 or} name {\cf19 in} tmp_characters_names:\par
00230                     {\cf19 return} {\cf18 None}, f{\cf22 "Error: Character name '\{name\}' already exists."}\par
00231                 description = lines[i + 2].strip()\par
00232 \par
00233                 parsed_data.append(CommandCreateCharacter(\par
00234                     name=name,\par
00235                     description=description\par
00236                 ))\par
00237                 tmp_characters_names.append(name)\par
00238                 i += 3\par
00239 \par
00240             {\cf19 elif} line == {\cf22 "Select character command."}:\par
00241                 {\cf19 if} i + 2 >= len(lines):\par
00242                     {\cf19 return} {\cf18 None}, {\cf22 "Error: Incomplete 'Select character command.'"}\par
00243                 name = lines[i + 1].strip()\par
00244                 {\cf19 if} name {\cf19 not} {\cf19 in} self.characters {\cf19 and} name {\cf19 not} {\cf19 in} tmp_characters_names:\par
00245                     {\cf19 return} {\cf18 None}, f{\cf22 "Error: Character name '\{name\}' does not exist."}\par
00246                 action = lines[i + 2].strip()\par
00247 \par
00248                 parsed_data.append(CommandSelectCharacter(\par
00249                     name=name,\par
00250                     action=action\par
00251                 ))\par
00252                 i += 3\par
00253 \par
00254             {\cf19 elif} line == {\cf22 "Describe environment command."}:\par
00255                 {\cf19 if} i + 1 >= len(lines):\par
00256                     {\cf19 return} {\cf18 None}, {\cf22 "Error: Incomplete 'The command to describe the environment.'"}\par
00257                 description = lines[i + 1].strip()\par
00258 \par
00259                 parsed_data.append(CommandDataEnvironmentDescription(\par
00260                     description=description\par
00261                 ))\par
00262                 i += 2\par
00263 \par
00264             {\cf19 elif} line == {\cf22 "Off-topic input command."}:\par
00265                 parsed_data.append(CommandOffTopic())\par
00266                 i += 1\par
00267 \par
00268             {\cf19 elif} line == {\cf22 "Player death command."}:\par
00269                 parsed_data.append(CommandPlayerDeath())\par
00270                 i += 1\par
00271 \par
00272             {\cf19 else}:\par
00273                 {\cf19 return} {\cf18 None}, f{\cf22 "Error: Unrecognized command '\{line\}'"}\par
00274         {\cf19 if} len(parsed_data) == 0:\par
00275             {\cf19 return} {\cf18 None}, f{\cf22 "Error: No commands found."}\par
00276 \par
00277         {\cf19 return} parsed_data, {\cf22 "No errors found."}\par
00278 \par
00279     {\cf17 def }parse_commands(self, commands: List[CommandData]) -> Tuple[str, Set[str]]:\par
00280         {\cf22 """!}\par
00281 {\cf22         @brief \'CE\'E1\'F0\'E0\'E1\'EE\'F2\'EA\'E0 \'F1\'EF\'E8\'F1\'EA\'E0 \'EA\'EE\'EC\'E0\'ED\'E4}\par
00282 {\cf22         }\par
00283 {\cf22         @param commands \'D1\'EF\'E8\'F1\'EE\'EA \'EA\'EE\'EC\'E0\'ED\'E4 \'E4\'EB\'FF \'EE\'E1\'F0\'E0\'E1\'EE\'F2\'EA\'E8}\par
00284 {\cf22         }\par
00285 {\cf22         @return Tuple[str, Set[str]] \'CA\'EE\'F0\'F2\'E5\'E6 \'E8\'E7 \'F2\'E5\'EA\'F1\'F2\'EE\'E2\'EE\'E3\'EE \'E2\'FB\'E2\'EE\'E4\'E0 \'E8 \'EC\'ED\'EE\'E6\'E5\'F1\'F2\'E2\'E0 \'E8\'EC\'E5\'ED \'E0\'EA\'F2\'E8\'E2\'ED\'FB\'F5 \'EF\'E5\'F0\'F1\'EE\'ED\'E0\'E6\'E5\'E9}\par
00286 {\cf22         }\par
00287 {\cf22         @throws PlayerDeathException \'E5\'F1\'EB\'E8 \'E2\'F1\'F2\'F0\'E5\'F7\'E5\'ED\'E0 \'EA\'EE\'EC\'E0\'ED\'E4\'E0 \'F1\'EC\'E5\'F0\'F2\'E8 \'E8\'E3\'F0\'EE\'EA\'E0}\par
00288 {\cf22         """}\par
00289         final_output = {\cf22 ""}\par
00290         active_characters: Set[str] = set()\par
00291         {\cf19 for} command {\cf19 in} commands:\par
00292             {\cf19 if} isinstance(command, CommandCreateCharacter):\par
00293                 self.characters[command.name] = command.description\par
00294                 self.db.save_character(self.session_id, command.name, command.description)\par
00295                 final_output += f{\cf22 "A new character appears: \{command.name\}. \{command.description\}\\n"}\par
00296                 active_characters.add(command.name)\par
00297                 \par
00298                 {\cf20 # Generate character portrait if enabled}\par
00299                 {\cf19 if} self.generate_character_images {\cf19 and} self.__image_manager:\par
00300                     self.__image_manager.generate_character_portrait(command.name, command.description)\par
00301                 \par
00302             {\cf19 elif} isinstance(command, CommandSelectCharacter):\par
00303                 final_output += f{\cf22 "\{command.name\}: \{command.action\}\\n"}\par
00304                 active_characters.add(command.name)\par
00305             {\cf19 elif} isinstance(command, CommandDataEnvironmentDescription):\par
00306                 final_output += f{\cf22 "Environment: \{command.description\}\\n"}\par
00307             {\cf19 elif} isinstance(command, CommandOffTopic):\par
00308                 final_output = {\cf22 "Input is off-topic"}\par
00309                 {\cf19 break}\par
00310             {\cf19 elif} isinstance(command, CommandPlayerDeath):\par
00311                 final_output += {\cf22 "The player character has died. The session has ended."}\par
00312                 {\cf19 raise} PlayerDeathException({\cf22 "Player character has died. Session ended."})\par
00313                 \par
00314         {\cf19 return} final_output, active_characters\par
}

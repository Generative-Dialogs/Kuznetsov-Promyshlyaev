00050 {\cf22         """}\par
00051         {\cf19 try}:\par
00052             {\cf20 # Convert the image data to a PIL Image}\par
00053             image = Image.open(BytesIO(image_data))\par
00054             \par
00055             {\cf20 # Save the image}\par
00056             image.save(target_path)\par
00057             \par
00058             logging.info(f{\cf22 "Successfully saved image to \{target_path\}"})\par
00059             {\cf19 return} target_path\par
00060             \par
00061         {\cf19 except} Exception {\cf17 as} e:\par
00062             logging.error(f{\cf22 "Error saving image: \{str(e)\}"})\par
00063             {\cf19 return} {\cf18 None}\par
00064 \par
00065     {\cf17 def }generate_image_response(self, prompt: str, target_path: str, character_images: Optional[List[str]] = {\cf18 None}) -> Optional[str]:\par
00066         {\cf22 """!}\par
00067 {\cf22         @brief \'C3\'E5\'ED\'E5\'F0\'E0\'F6\'E8\'FF \'E8\'E7\'EE\'E1\'F0\'E0\'E6\'E5\'ED\'E8\'FF \'F1 \'E8\'F1\'EF\'EE\'EB\'FC\'E7\'EE\'E2\'E0\'ED\'E8\'E5\'EC Gemini API}\par
00068 {\cf22         }\par
00069 {\cf22         @param prompt \'D2\'E5\'EA\'F1\'F2\'EE\'E2\'FB\'E9 \'EF\'F0\'EE\'EC\'EF\'F2 \'E4\'EB\'FF \'E3\'E5\'ED\'E5\'F0\'E0\'F6\'E8\'E8 \'E8\'E7\'EE\'E1\'F0\'E0\'E6\'E5\'ED\'E8\'FF}\par
00070 {\cf22         @param target_path \'CF\'EE\'EB\'ED\'FB\'E9 \'EF\'F3\'F2\'FC \'E4\'EB\'FF \'F1\'EE\'F5\'F0\'E0\'ED\'E5\'ED\'E8\'FF \'E8\'E7\'EE\'E1\'F0\'E0\'E6\'E5\'ED\'E8\'FF}\par
00071 {\cf22         @param character_images \'CE\'EF\'F6\'E8\'EE\'ED\'E0\'EB\'FC\'ED\'FB\'E9 \'F1\'EF\'E8\'F1\'EE\'EA \'EF\'F3\'F2\'E5\'E9 \'EA \'E8\'E7\'EE\'E1\'F0\'E0\'E6\'E5\'ED\'E8\'FF\'EC \'EF\'E5\'F0\'F1\'EE\'ED\'E0\'E6\'E5\'E9}\par
00072 {\cf22         }\par
00073 {\cf22         @return Optional[str] \'CF\'F3\'F2\'FC \'EA \'F1\'EE\'F5\'F0\'E0\'ED\'E5\'ED\'ED\'EE\'EC\'F3 \'E8\'E7\'EE\'E1\'F0\'E0\'E6\'E5\'ED\'E8\'FE \'E8\'EB\'E8 None \'E2 \'F1\'EB\'F3\'F7\'E0\'E5 \'EE\'F8\'E8\'E1\'EA\'E8}\par
00074 {\cf22         """}\par
00075         {\cf19 try}:\par
00076             {\cf20 # Prepare contents list with prompt and images}\par
00077             contents: List[Any] = [prompt]\par
00078             \par
00079             {\cf20 # Add character images if provided}\par
00080             {\cf19 if} character_images:\par
00081                 {\cf19 for} image_path {\cf19 in} character_images:\par
00082                     {\cf19 try}:\par
00083                         image = Image.open(image_path)\par
00084                         contents.append(image)\par
00085                     {\cf19 except} Exception {\cf17 as} e:\par
00086                         logging.warning(f{\cf22 "Failed to load character image \{image_path\}: \{str(e)\}"})\par
00087             \par
00088             {\cf20 # Generate the image using Gemini API}\par
00089             response = self.__client.models.generate_content(\par
00090                 model={\cf22 'gemini-2.0-flash-exp-image-generation'},\par
00091                 contents=contents,\par
00092                 config=types.GenerateContentConfig(\par
00093                     response_modalities=[{\cf22 'Text'}, {\cf22 'Image'}]\par
00094                 )\par
00095             )\par
00096             \par
00097             {\cf20 # Process the response}\par
00098             {\cf19 for} part {\cf19 in} response.candidates[0].content.parts:\par
00099                 {\cf19 if} part.inline_data {\cf19 is} {\cf19 not} {\cf18 None}:\par
00100                     {\cf19 return} self.__save_image(part.inline_data.data, target_path)\par
00101                     \par
00102             logging.error({\cf22 "No image data found in response"})\par
00103             {\cf19 return} {\cf18 None}\par
00104             \par
00105         {\cf19 except} Exception {\cf17 as} e:\par
00106             logging.error(f{\cf22 "Error generating image: \{str(e)\}"})\par
00107             {\cf19 return} {\cf18 None} \par
}
